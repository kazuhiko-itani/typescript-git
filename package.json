{
  "name": "typescript-git",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "exec": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint .",
    "prettier": "prettier --check .",
    "fmt": "prettier --write .",
    "typecheck": "tsc -p . --noEmit"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.3",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "commander": "^9.1.0",
    "configparser": "^0.3.9",
    "dotenv": "^16.0.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.jest.json"
      }
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "setupFiles": [
      "./setupFiles.ts"
    ],
    "testRegex": "/__tests__/.*\\.ts$",
    "modulePathIgnorePatterns": [
      "/__tests__/test_helpers/*"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  }
}
